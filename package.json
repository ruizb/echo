{
  "name": "echo",
  "version": "0.1.0",
  "description": "Online hearing experiment.",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && NODE_ENV=development node src/server.js",
    "build": "npm run clean && NODE_ENV=production parcel build src/client/*.html --public-url ./ && npm run build-functions",
    "lambda:start": "netlify-lambda serve src/functions",
    "lambda:build": "netlify-lambda build src/functions",
    "clean": "rm -rf ./dist",
    "test": "jest"
  },
  "author": "Benoit Ruiz",
  "license": "ISC",
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.1",
    "@types/aws-lambda": "^8.10.53",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.3.38",
    "@types/parse-filepath": "^1.0.0",
    "@types/semantic-ui": "^2.2.7",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "netlify-lambda": "^1.6.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.1.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "parse-filepath": "^1.0.2"
  }
}
